{
	"name": "media-summarizer",
	"version": "0.1.0-beta",
	"description": "View YouTube videos in a custom pane while taking notes, with AI-powered transcript summarization using multiple AI providers (OpenAI, OpenRouter, Ollama).",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"youtube",
		"summarizer",
		"ai",
		"openai",
		"openrouter",
		"ollama",
		"transcript",
		"multi-provider"
	],
	"author": "Jonathan Horst",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^20.14.0",
		"@types/react": "^18.2.42",
		"@types/react-dom": "^18.2.17",
		"@typescript-eslint/eslint-plugin": "^7.18.0",
		"@typescript-eslint/parser": "^7.18.0",
		"builtin-modules": "^4.0.0",
		"esbuild": "^0.25.5",
		"obsidian": "latest",
		"tslib": "^2.6.3",
		"typescript": "^5.5.4"
	},
	"dependencies": {
		"youtube-transcript": "^1.0.6",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-youtube": "^10.1.0"
	}
}